{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.*",
      "coverage/lcov-report/*.js",
      "src/proto/**/*.*"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"],
    "no-console": false,
    "jsx-no-lambda": false,
    "member-ordering": false,
    "no-empty": false
  }
}
